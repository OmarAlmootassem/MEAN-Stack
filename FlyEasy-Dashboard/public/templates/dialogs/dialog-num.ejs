<md-dialog aria-label="Edit Info">
  <md-toolbar>
    <div class="md-toolbar-tools">
      <h2 style="color: white">Edit Info</h2>
    </div>
  </md-toolbar>

  <md-dialog-content>
    <div class="md-dialog-content">
      <md-input-container md-no-float class="md-block">
        <input type="number" min="0" name="seats" ng-model="info" md-autofocus>
      </md-input-container>
    </div>
  </md-dialog-content>
  <md-dialog-actions layout="row">
    <md-button ng-click="cancel()">Cancel</md-button>
    <md-button class='md-primary' ng-click="saveInfo()">Save</md-button>
  </md-dialog-actions>
</md-dialog>